<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="orbital.gns.pocketalert">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".StatusUpdates.FriendsStatus">
        </activity>
        <activity android:name=".StatusUpdates.StatusUpdateActivity">
        </activity>
        <activity android:name=".Friends.FriendsListActivity">
        </activity>
        <activity android:name=".Friends.FriendsProfileActivity">
        </activity>
        <activity android:name=".Friends.AddFriendsActivity">
        </activity>
        <activity android:name=".Profile.MyProfileActivity">
        </activity>
        <activity android:name=".Opening.MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Opening.RegisterActivity">
        </activity>
        <activity android:name=".Opening.LoginActivity">
        </activity>
    </application>

</manifest>